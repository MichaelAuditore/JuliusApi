{
  "name": "juliusapi",
  "version": "1.0.0",
  "description": "API to create posts using an authenticated user existing in DB",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MichaelAuditore/JuliusApi.git"
  },
  "author": "Miguel A. Parada [michael_v613@hotmail.com]",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MichaelAuditore/JuliusApi/issues"
  },
  "homepage": "https://github.com/MichaelAuditore/JuliusApi#readme",
  "engines": {
    "node": "10",
    "npm": "6"
  },
  "scripts": {
    "build": "babel src --out-dir dist",
    "test": "cross-env NODE_ENV=testing jest --forceExit --detectOpenHandles  --silent",
    "test-routes": "npm run test -t router",
    "test-models": "npm run test -t model",
    "test-controllers": "npm run test -t controllers",
    "test-auth": "npm run test -t Authentication:",
    "dev": "nodemon --exec npm run restart",
    "prod": "cross-env NODE_ENV=prod && nodemon --exec npm run restart",
    "restart": "rimraf dist && npm run build",
    "start": "npm run restart && node dist/index.js"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "cuid": "^2.1.4",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.11",
    "mongoose": "^5.3.13",
    "morgan": "^1.9.1",
    "rimraf": "^2.6.2",
    "validator": "^10.9.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^26.3.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.15.1",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "jest": "^26.4.2",
    "mock-req-res": "^1.0.2",
    "nodemon": "^2.0.4",
    "prettier": "^1.15.2",
    "supertest": "^3.3.0"
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/test-db-setup.js"
    ],
    "testPathIgnorePatterns": [
      "dist/"
    ],
    "restoreMocks": true
  }
}
